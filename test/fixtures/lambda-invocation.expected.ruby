(raise "Assertion failed" unless ->(double) { double.call(5) }.call(->(x) { x * 2 }) == 10; true)
(raise "Assertion failed" unless ->(add) { add.call(3, 4) }.call(->(x, y) { x + y }) == 7; true)
(raise "Assertion failed" unless ->(mul) { mul.call(6, 7) }.call(->(a, b) { a * b }) == 42; true)
(raise "Assertion failed" unless ->(abs) { abs.call(-5) }.call(->(x) { x * 2 }) == -10; true)
(raise "Assertion failed" unless ->(round) { round.call(10) }.call(->(x) { x + 1 }) == 11; true)
(raise "Assertion failed" unless ->(length) { length.call("ignored") }.call(->(s) { 42 }) == 42; true)
(raise "Assertion failed" unless ->(nested) { nested.call(5) }.call(->(x) { ->(inner) { inner.call(x) + 1 }.call(->(y) { y * 2 }) }) == 11; true)
(raise "Assertion failed" unless ->(toUpper) { toUpper.call("hello") }.call(->(s) { s.upcase }) == "HELLO"; true)
(raise "Assertion failed" unless ->(toLower) { toLower.call("HELLO") }.call(->(s) { s.downcase }) == "hello"; true)
(raise "Assertion failed" unless ->(strLen) { strLen.call("hello") }.call(->(s) { s.length }) == 5; true)
